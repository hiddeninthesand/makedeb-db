#!/usr/bin/env bash
##
# Package-specific dependencies. These get added to the list of preexisting
# dependencies in the PKGBUILD.
##
#

atom() {
  arch_packages=('apm' 'electron9')
  add_conflicts=('atom')
}

electron9() {
  arch_packages=('icu' 'libwebp')
  add_depends=('icu' 'libwebp')
}

element_desktop() {
  arch_packages=('element-web')
}

unexicon_sound_theme_git() {
  remove_makedepends=('pkgconfig')
  add_makedepends=('libpkgconf')
}

yq() {
  add_depends=('python-is-python3')
  add_makedepends=('python-is-python3')
}

