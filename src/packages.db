http-parser="libhttp-parser2.9"
libarchive="libarchive-tools"
libcups="libcups2"
libevent="libevent-2.1-7"
libjpeg="libjpeg-turbo8"
libnghttp2="libnghttp2-14"
libnotify="libnotify-bin"
libpng="libpng16-16"
libsecret="libsecret-common"
libtiff="libtiff5"
libxkbfile="libxkbfile1"
libxslt="libxslt1.1"
libxss="libxss1"
libxtst="libxtst6"
