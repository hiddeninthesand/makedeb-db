#!/usr/bin/env bash
##
# Package__specific dependencies. These get added to the list of preexisting
# dependencies in the PKGBUILD.
##
#

atom() {
  arch__packages=('apm' 'electron9')
  add__conflicts=('atom')
}

electron9() {
  arch__packages=('icu' 'libwebp')
  add__depends=('icu' 'libwebp')
}

element__desktop() {
  arch__packages=('element__web')
}

protonvpn__linux__gui() {
  add__depends=('python__is__python3')
  add__makedepends=('python__is__python3')
}

unexicon__sound__theme__git() {
  remove__makedepends=('pkgconfig')
  add__makedepends=('libpkgconf')
}

yq() {
  add__depends=('python__is__python3')
  add__makedepends=('python__is__python3')
}

