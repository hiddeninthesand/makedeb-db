#!/usr/bin/env bash
##
# Package__specific dependencies. These get added to the list of preexisting
# dependencies in the PKGBUILD.
##
#

atom() {
    arch_packages=('apm' 'electron9')
    add_conflicts=('atom')
}

electron9() {
    arch_packages=('icu' 'libwebp')
    add_depends=('icu' 'libwebp')
}

element__desktop() {
    arch_packages=('element__web')
}

element__web() {
    arch_packages=('electron' 'nodejs__lts__fermium')
}

protonvpn__linux__gui() {
    add_depends=('python__is__python3')
    add_makedepends=('python__is__python3')
}

unexicon__sound__theme__git() {
    remove_makedepends=('pkgconfig')
    add_makedepends=('libpkgconf')
}

yq() {
    add_depends=('python__is__python3')
    add_makedepends=('python__is__python3')
}

